{
	"manifest_version": 2,
	"name": "Singular",
	"version": "1.0",
	"description": "Singular",
	"icons": {
    	"48": "icon48.png",
        "128": "icon128.png"
    },
	"permissions":[
		"tabs",
		"activeTab",
    	"storage",
		"notifications",
		"https://*/*",
		"http://*/*"
	],
	"browser_action" : {
		"default_popup": "index.html"
	},
	"content_scripts": [
	    {
	      "matches": ["*://*/*"],
	      "js": ["jquery.js", "domReader.js", "notify.min.js"]
		}
	]
	
}